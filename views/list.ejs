<%- include("header") -%>
  <div class="box" id="heading">
      <h1> <%= listOfTitle %> </h1>
  </div>

  <div class="box">
    <%  for(let i=0;i<newListItems.length ;i++){ %>
    <div class="item">
      <input type="checkbox">
      <p><%=newListItems[i]%></p>
    </div>
    <%}  %>
    <form class="item"action="/" method="post">
      <input type="text" name="newItem" placeholder="Add here">
      <button type="submit" name="list" value=<%=listOfTitle%>>+</button>
    </form>
  </div>

<%- include("footer") -%>
